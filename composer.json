{
  "name": "ksfraser/fa-product-attributes",
  "version": "0.1.0",
  "description": "FrontAccounting-compatible ordered product attributes (royal order of adjectives)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "KS Fraser"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ksfraser/html": "^1.0",
    "ksfraser/ksf-modules-dao": "0.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "9.4.*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/ksfraser/origin"
    },
    {
      "type": "git",
      "url": "https://github.com/ksfraser/ksf_ModulesDAO"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ksfraser\\FA_ProductAttributes\\": "composer-lib/src/Ksfraser/FA_ProductAttributes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ksfraser\\FA_ProductAttributes\\Test\\": "composer-lib/tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
