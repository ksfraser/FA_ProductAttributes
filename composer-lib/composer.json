{
  "name": "ksfraser/fa-product-attributes",
  "version": "0.1.0",
  "description": "FrontAccounting-compatible ordered product attributes (royal order of adjectives)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "KS Fraser"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ksfraser/html": "^1.0",
    "ksfraser/fa-hooks": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "nikic/php-parser": "^4.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ksfraser/origin"
    },
    {
      "type": "path",
      "url": "../fa-hooks",
      "options": {
        "symlink": false
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "Ksfraser\\FA_ProductAttributes\\": "src/Ksfraser/FA_ProductAttributes/"
    }
  },
  "config": {
    "platform": {
      "php": "7.3.21"
    }
  }
}
