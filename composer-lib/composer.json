{
  "name": "ksfraser/fa-product-attributes",
  "version": "0.1.0",
  "description": "FrontAccounting-compatible ordered product attributes (royal order of adjectives)",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "KS Fraser"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ksfraser/fa-hooks": "*",
    "ksfraser/ksf-modules-dao": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "nikic/php-parser": "^4.0"
  },
  "repositories": [
  ],
  "autoload": {
    "psr-4": {
      "Ksfraser\\FA_ProductAttributes\\": "src/Ksfraser/FA_ProductAttributes/",
      "Ksfraser\\FA_ProductAttributes_Variations\\": "../fa_product_attributes_variations/src/Ksfraser/FA_ProductAttributes_Variations/",
      "Ksfraser\\FA_Hooks\\": "../fa-hooks/src/Ksfraser/FA_Hooks/",
      "Ksfraser\\ModulesDAO\\": "../ksf-modules-dao/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ksfraser\\FA_ProductAttributes\\Test\\": "tests/",
      "Ksfraser\\FA_ProductAttributes_Variations\\Test\\": "../fa_product_attributes_variations/tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    }
  }
}
